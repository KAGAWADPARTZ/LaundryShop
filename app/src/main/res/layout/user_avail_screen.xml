<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="match_parent"
        android:layout_height="594dp"
        android:layout_marginBottom="200dp"
        app:layout_constraintBottom_toTopOf="@+id/button3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/design1" />

    <RelativeLayout
        android:id="@+id/relativeLayout5"
        android:layout_width="379dp"
        android:layout_height="526dp"
        android:background="#FFFFFF"
        android:outlineAmbientShadowColor="#252323"
        android:outlineSpotShadowColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="46dp"
            android:layout_marginBottom="478dp"
            android:background="#C9C4C4"
            android:orientation="horizontal"></LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="415dp"
            android:layout_marginBottom="109dp"
            android:background="#C9C4C4"
            android:orientation="horizontal"></LinearLayout>

        <TextView
            android:id="@+id/textView7"
            android:layout_width="78dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="19dp"
            android:layout_marginEnd="282dp"
            android:layout_marginBottom="480dp"
            android:text="List Service"
            android:textColor="#3DC2EC" />

        <TextView
            android:id="@+id/qty"
            android:layout_width="32dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="140dp"
            android:layout_marginEnd="207dp"
            android:layout_marginBottom="480dp"
            android:text="Qty"
            android:textColor="#3DC2EC" />

        <TextView
            android:id="@+id/totalprice"
            android:layout_width="49dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="34dp"
            android:layout_marginEnd="296dp"
            android:layout_marginBottom="80dp"
            android:text="Total"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/wash"
            android:layout_width="49dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="33dp"
            android:layout_marginEnd="297dp"
            android:layout_marginBottom="440dp"
            android:text="Wash"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/wash2"
            android:layout_width="13dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="150dp"
            android:layout_marginEnd="216dp"
            android:layout_marginBottom="439dp"
            android:text="2"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/wash3"
            android:layout_width="20dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="255dp"
            android:layout_marginEnd="104dp"
            android:layout_marginBottom="439dp"
            android:text="45"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/wash4"
            android:layout_width="21dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="339dp"
            android:layout_marginEnd="19dp"
            android:layout_marginBottom="435dp"
            android:text="90"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/price"
            android:layout_width="38dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="246dp"
            android:layout_marginEnd="95dp"
            android:layout_marginBottom="481dp"
            android:text="Price"
            android:textColor="#3DC2EC" />

        <TextView
            android:id="@+id/total"
            android:layout_width="39dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="330dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="477dp"
            android:text="total"
            android:textColor="#3DC2EC" />
    </RelativeLayout>

    <TextView
        android:id="@+id/profileName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:text="Rowel"
        android:textColor="#96D5E9"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/btn_avail"
        android:layout_width="112dp"
        android:layout_height="51dp"
        android:layout_marginBottom="100dp"
        android:background="@drawable/round_button"
        android:text="Avail"
        android:textColor="@color/white"
        android:textStyle="bold"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/btn_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:backgroundTint="#8AD9F3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_arrow_back_24" />

</androidx.constraintlayout.widget.ConstraintLayout>