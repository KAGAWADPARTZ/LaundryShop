<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:background="@color/lightblue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <ImageView
            android:id="@+id/logo"
            android:layout_width="204dp"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="96dp"
            android:layout_marginEnd="111dp"
            android:src="@drawable/minilogo" />

    </RelativeLayout>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="🤝 𝙏𝙧𝙖𝙣𝙨𝙖𝙘𝙩𝙞𝙤𝙣 𝙈𝙖𝙙𝙚 🤝"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.171" />

    <RelativeLayout
        android:id="@+id/linearLayout6"
        android:layout_width="411dp"
        android:layout_height="72dp"
        android:background="#D9D9D9"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <ImageButton
            android:id="@+id/btn_laundry"
            android:layout_width="53dp"
            android:layout_height="48dp"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="3dp"
            android:layout_marginEnd="40dp"
            android:layout_marginRight="30dp"
            android:layout_toStartOf="@+id/btn_status"
            android:foreground="@drawable/baseline_local_laundry_service_24"
            android:foregroundTint="#5F7B83"
            app:tint="#3DC2EC" />

        <ImageButton
            android:id="@+id/btn_status"
            android:layout_width="50dp"
            android:layout_height="52dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="1dp"
            android:layout_marginEnd="46dp"
            android:layout_marginRight="30dp"
            android:layout_marginBottom="19dp"
            android:layout_toStartOf="@+id/btn_history"
            android:foreground="@drawable/baseline_library_add_24"
            android:foregroundTint="#5F7B83"
            app:tint="#5F7B83" />

        <ImageButton
            android:id="@+id/btn_history"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="38dp"
            android:layout_marginRight="30dp"
            android:layout_toStartOf="@+id/btn_profile"
            android:foreground="@drawable/baseline_history_24"
            android:foregroundTint="#5F7B83"
            app:tint="#5F7B83" />

        <ImageButton
            android:id="@+id/btn_profile"
            android:layout_width="46dp"
            android:layout_height="49dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="25dp"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="23dp"
            android:foreground="@drawable/baseline_account_circle_24"
            android:foregroundTint="#5F7B83"
            app:tint="#5F7B83" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/btn_laundry"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="3dp"
            android:layout_marginEnd="305dp"
            android:layout_marginBottom="-9dp"
            android:text="𝙇𝙖𝙪𝙣𝙙𝙧𝙮"
            android:textColor="#FF0000"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/btn_laundry"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="217dp"
            android:layout_marginBottom="-11dp"
            android:text="𝙎𝙩𝙖𝙩𝙪𝙨"
            android:textColor="#FF0000"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/btn_laundry"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="120dp"
            android:layout_marginBottom="-10dp"
            android:text="𝙃𝙞𝙨𝙩𝙤𝙧𝙮"
            android:textColor="#FF0000"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/btn_laundry"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="3dp"
            android:layout_marginEnd="33dp"
            android:layout_marginBottom="-10dp"
            android:text="𝙋𝙧𝙤𝙛𝙞𝙡𝙚"
            android:textColor="#FF0000"
            android:textSize="10sp" />
    </RelativeLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv"
        android:layout_width="389dp"
        android:layout_height="470dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.648" />


    <!--User Home Screen-->

    <!--User Status Screen-->

    <!--User History Screen -->


</androidx.constraintlayout.widget.ConstraintLayout>